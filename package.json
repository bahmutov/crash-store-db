{
  "name": "crash-store-db",
  "description": "Actual data store for reported JavaScript exceptions",
  "main": "src/store-api.js",
  "version": "0.0.0-semantic-release",
  "dependencies": {
    "bluebird": "3.0.5",
    "check-more-types": "2.1.2",
    "kerberos": "0.0.17",
    "lazy-ass": "1.0.0",
    "mongoose": "4.2.5",
    "nconf": "0.8.2"
  },
  "devDependencies": {
    "eslint": "1.9.0",
    "git-issues": "1.1.0",
    "pre-git": "1.2.11",
    "semantic-release": "^4.3.5"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint index.js src/**/*.js",
    "commit": "git-issues && commit-wizard",
    "issues": "git-issues",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/crash-store-db.git"
  },
  "keywords": [
    "crash",
    "exception",
    "error",
    "runtime",
    "store",
    "db"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahmutov/crash-store-db/issues"
  },
  "homepage": "https://github.com/bahmutov/crash-store-db#readme",
  "config": {
    "pre-git": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": [
        "npm run lint"
      ],
      "pre-push": [],
      "post-commit": [],
      "post-merge": []
    }
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
}
